import java.io.*;
import java.util.*;
class right_triangle
{
	public static void main(String args[])
	{
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int num=1;
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				if(i>=j)
				{
					System.out.print(num+ " ");
					num++;
				}
			}
			System.out.println();
		}
	}
}
